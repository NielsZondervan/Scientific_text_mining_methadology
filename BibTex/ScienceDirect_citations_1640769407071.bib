year = {2018},
isbn = {978-0-08-102171-2},
doi = {https://doi.org/10.1016/B978-0-08-102171-2.00020-9},
url = {https://www.sciencedirect.com/science/article/pii/B9780081021712000209},
author = {Megan Hodge},
keywords = {Games, Partnerships, Targeting Specific Audiences, Tours},
abstract = {The English Language Program (ELP) faculty at Virginia Commonwealth University (VCU) are eager to familiarize their students with the services and resources offered by the VCU Library. These students' proficiency in English is often limited, meaning a traditional tour or presentation is out of the question, and physically visiting each of the chosen resources as opposed to merely discussing them in the abstract is critical. To best meet the unique needs of these students, an Amazing Race–style orientation to the library was created. This chapter discusses the logistics of creating a library orientation for English language learners in an Amazing Race style; describes the pedagogical benefits of using an interactive orientation for this patron demographic; relates the hands-on activities used as “challenges”; interprets feedback from participants, team leaders, and ELP faculty; explains what worked well and could be adapted by other libraries for other patron types; and shares lessons learned.}
}
@article{JOSHI20184787,
title = {Modelling and Product customization of PVC Coating Machine},
