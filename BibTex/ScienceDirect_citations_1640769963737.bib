author = {Lisa N. Britton and Matthew J. Cicoria},
keywords = {Accounting, insurance, telemedicine, secure video conferencing, media creation},
abstract = {This chapter outlines several logistical considerations that need to be examined before one begins to supervise trainees. We strongly suggest that supervisors consider whether they need to: (1) create a separate business structure in order to provide supervision; (2) obtain insurance that covers supervisory activities; and (3) get assistance to understand, where applicable, the tax implications of providing supervision independently. Because this book focuses on remote supervision, we also discuss the need to communicate securely with trainees. Lastly, we make suggestions for creating training media, in advance of undertaking supervision, that will make the supervisory process easier in the long run.}
}
@article{HASSAN2021100363,
title = {Performance assessment of high capacity trucks: Understanding truck selection and deployment economics},
journal = {Transportation Research Interdisciplinary Perspectives},
volume = {10},
pages = {100363},
year = {2021},
issn = {2590-1982},
doi = {https://doi.org/10.1016/j.trip.2021.100363},
url = {https://www.sciencedirect.com/science/article/pii/S2590198221000701},
author = {Tajudeen Hassan and Petri Helo},
keywords = {High capacity transportation vehicles, Performance evaluation, Logistics},
abstract = {High capacity trucks (HCT) present an interesting opportunity to improve transportation efficiency and reduce emissions. This study focuses on implementation in Finland where legislation allows a maximum weight of 76 tonnes, 34.5 m length and 4.4 m height, which would be a 20% and 4.5% increase in weight and height compared to the current European modular system. The purpose of this paper is to evaluate the economic performance (cost and revenue) of such high capacity transportation vehicles compared to traditional smaller trucks. Data has been collected from real transport logistics service providers. A performance evaluation model called COREPE was designed to present quantitative evaluation of one year of operating data: this model evaluates the economic performance of HCT and traditional trucks on three different long hauls using telemetry data and monthly truck operating data. The results show that HCT has overall higher cost compared to traditional. The size advantage HCT has over traditional translated into moderately higher revenue and profitability based on the available data. Factors such as seasonal variability, driver attitude and truck utilisation had a noticeable impact on cost.}
}
@article{CHU2019620,
title = {Managing interdependence: Using Guanxi to cope with supply chain dependency},
journal = {Journal of Business Research},
volume = {103},
pages = {620-631},
year = {2019},
issn = {0148-2963},
doi = {https://doi.org/10.1016/j.jbusres.2017.11.035},
url = {https://www.sciencedirect.com/science/article/pii/S0148296317304812},
author = {Zhaofang Chu and Qiang Wang and Fujun Lai and Brian J. Collins},
keywords = {, Logistics outsourcing, Dependency, China},
abstract = {The continued growth of logistics outsourcing and dependence on third-party logistics (3PL) providers highlights the need for firms to strategically consider their dependence on their supply chain. We draw on resource dependency theory to propose guanxi (a cultural tradition in China of interpersonal connections that facilitate a mutual exchange of favors) as a dependency-coping strategy. Integrating transaction cost economics, we propose the effectiveness of guanxi as a dependence coping strategy is contingent on the level of exchange hazards (i.e., uncertainty and asset specificity) in logistics outsourcing. Based on data collected from 149 3PL users in China, we found that guanxi is a more potent strategy in highly uncertain environments and less relevant when extensive relationship-specific assets are involved. Implications and directions of future research are discussed.}
}
@article{ENSTROM2021106182,
title = {Wood chip supply from forest to port of loading – A simulation study},
