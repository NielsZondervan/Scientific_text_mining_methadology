volume = {152},
pages = {106182},
year = {2021},
issn = {0961-9534},
doi = {https://doi.org/10.1016/j.biombioe.2021.106182},
url = {https://www.sciencedirect.com/science/article/pii/S096195342100218X},
author = {Johanna Enström and Anders Eriksson and Lars Eliasson and Aron Larsson and Leif Olsson},
keywords = {Discrete-event simulation (DES), Forest fuel, Logistics, Maritime transport, Short sea shipping, Terminal logistics, Supply chain},
abstract = {Maritime transport facilitates trade with bioenergy feedstocks in the Baltic region. The study aims to provide guidance on efficient organisation of logistics at the port of loading for maritime transport of wood chips. The economic effects of using an intermediate terminal as opposed to direct delivery to port were studied, as well as the relationships between production capacity and storage capacity. Discrete-Event Simulation was used to analyse scenarios where a variable part of the volume is handled through the terminal. The total supply cost, including chipping, transportation, storage and handling at inland terminal and port, as well as loading of the ship, varied between €6.73 and 7.85 per MWh in the different scenarios. The volume passing through the terminal had a significant influence on total cost, showing a supply chain cost increase of €0.78 per m3 (approximately €4.67 per dry tonne) for material delivered through the terminal. The difference in storage cost between port and inland terminal determines whether the terminal volumes affect costs, which was shown by a sensitivity analysis. Even so, the terminal offers a possibility to manage uncertainty, both in production rates and in shipping date, and influences the supply network. The main advantage of using a simulation technique for planning production and logistic flows is the visualisation of risks and margins.}
}
@article{ANITHA2021,
title = {IoT’s in enabling the supply chain visibility and connectivity and optimization of performance},
journal = {Materials Today: Proceedings},
year = {2021},
issn = {2214-7853},
doi = {https://doi.org/10.1016/j.matpr.2020.12.343},
url = {https://www.sciencedirect.com/science/article/pii/S2214785320400598},
author = {Komma Anitha and K. {Palaksha Reddy} and N. Krishnamoorthy and Sushma Jaiswal},
keywords = {Supply chain, IIoT, Connectivity, Visibility, Logistics 4.0, Industry 4.0},
abstract = {New innovations and entrepreneurial prospects emerge with the fourth industry revolution, which enables previously unknown opportunities to boost efficiency within the industrialised and its supply chains. The key player in industry 4.0: the Internet of Things (IoT), by improving communication and visibility across the entire logistics value chain. IoT is however still in its early steps, leaving confusion about future areas of application and implications in the manufacturing sector and their supply chain environment. The goal of this work is therefore the role of ioT technology in enabling the visibility and connectivity of the supply chain and its impact on the performance of the supply chain. The analysis of the collected data provided a summary of the 10 areas in which knowledge needs within a linked end-to-end supply chain are described, how Iot technology can be used to fulfil these requirements, and the general effect on the efficiency of the supply chain of increased visibility.}
}
@article{RASHIDI2019266,
title = {A comparison of fuzzy DEA and fuzzy TOPSIS in sustainable supplier selection: Implications for sourcing strategy},
journal = {Expert Systems with Applications},
volume = {121},
pages = {266-281},
year = {2019},
issn = {0957-4174},
doi = {https://doi.org/10.1016/j.eswa.2018.12.025},
url = {https://www.sciencedirect.com/science/article/pii/S0957417418307930},
author = {Kamran Rashidi and Kevin Cullinane},
keywords = {DEA, TOPSIS, Sourcing, Suppliers, Sustainability, Logistics},
abstract = {This paper presents a comparative analysis of the outcomes achieved when two widely applied methods for supplier selection—the ‘technique for order of preference by similarity to ideal solution’ (TOPSIS) and ‘data envelopment analysis’—are applied to the problem of identifying the most preferred sustainable suppliers. Both fuzzy DEA and fuzzy TOPSIS are applied to a common dataset of logistics service providers in Sweden. The results reveal that TOPSIS outperforms DEA in terms of both calculation complexity and sensitivity to changes in the number of suppliers. However, output rankings from the two models are found to be less than perfectly correlated. The paper concludes that utilizing both methods, as applied to just a small number of evaluation criteria and a relatively low level of detail in the data, produces a useful pooled shortlist of potential sustainable suppliers. This can then form the basis for a second stage application where either of the methods may be applied to a greater number of criteria that are specified to a higher level of detail. Even more critically, the results also have the potential to point to specific aspects for discussion when negotiating price and service quality commitments with potential sustainable suppliers.}
}
@article{ABBASI2021107019,
title = {Reliable supply chain network design for 3PL providers using consolidation hubs under disruption risks considering product perishability: An application to a pharmaceutical distribution network},
journal = {Computers & Industrial Engineering},
volume = {152},
pages = {107019},
year = {2021},
issn = {0360-8352},
doi = {https://doi.org/10.1016/j.cie.2020.107019},
url = {https://www.sciencedirect.com/science/article/pii/S0360835220306896},
author = {Sepehr Abbasi and Ali Saboury and Mohammad Saeed Jabalameli},
keywords = {Third-party logistics, Reliable network design, Hub location problem, Disruption, Perishability},
abstract = {Today, organizations outsource their logistics activities to third-party logistics (3PL) providers to focus more on their key capabilities. A 3PL company is an external provider that controls, manages, and provides logistics services for the manufacture. The success of a 3PL company tremendously depends on the design of its logistics network. This paper studies the problem of designing a reliable hub-and-spoke network using consolidation hubs considering disruption risks and product perishability. To lessen network failure risks, disruption is dealt with using backup facilities, fortification of facilities, and multiple sourcing simultaneously. This problem is formulated as a bi-objective MINLP model that decides on the location, number, and fortification of main and backup hubs and distribution centers, the location of backup facilities, the inventory of perishable products and the back-ordering of products to minimize the cost and time of the supply chain network simultaneously. One of the highlights of this model is that disruption of hubs and distribution centers, which have the role of storing and distributing products, affects product perishability because the storing and distributing conditions and requirements are not met under facilities disruption. The model is linearized, and the uncertainty of facility disruption probabilities is dealt with by using credibility-based possibilistic programming so that an optimal solution can be provided. A major pharmaceutical distribution 3PL company in Iran is considered as a practical application of the model to validate and solve it. Numerical results reveal the relation between product perishability and disruption of the storing facilities and the 3PL policy in distribution, storage, back-ordering, and fortification.}
}
