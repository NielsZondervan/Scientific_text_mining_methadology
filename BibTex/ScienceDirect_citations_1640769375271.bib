url = {https://www.sciencedirect.com/science/article/pii/B9780081005965031760},
author = {Gérald Cavalier},
keywords = {Energy consumption, F-gas, Insulation, Performance, Refrigeration, Sustainability, Transport},
abstract = {Developments in refrigerated transport equipment (Heap, 2010) have been strongly linked, over the last 60years, to the development of food exchange worldwide. Food safety can now be achieved throughout transport and logistics operations that were, for decades, the critical point of the cold chain. Nevertheless, these developments are not universal, and in some parts of the world where food losses and their environmental and social impacts remain very high, refrigerated transport is still limited. Operators and designers of refrigerated transport systems are now also seeking technical solutions to the challenge of improved sustainability and environmental performance. This article focuses on the economic, environmental, and social impact of refrigerated transport and the technical challenges facing refrigerated transport equipment designers, manufacturers, users, or experts to make the sector sustainable.}
}
@article{GIONES2017875,
title = {From toys to tools: The co-evolution of technological and entrepreneurial developments in the drone industry},
journal = {Business Horizons},
volume = {60},
number = {6},
pages = {875-884},
year = {2017},
note = {THE GENERATIVE POTENTIAL OF EMERGING TECHNOLOGY},
issn = {0007-6813},
doi = {https://doi.org/10.1016/j.bushor.2017.08.001},
url = {https://www.sciencedirect.com/science/article/pii/S0007681317301210},
author = {Ferran Giones and Alexander Brem},
keywords = {Technological progress, Entrepreneurial activity, Drones, Drone industry, Emerging industries, Unmanned aircraft systems, Unmanned aerial vehicles, Drone development},
abstract = {There is undoubtedly hype around drones and their applications for private and professional users. Based on a brief overview of the development of the drone industry in recent years, this article examines the co-evolution of drone technology and the entrepreneurial activity linked to it. Our results highlight the industry emergence described as concept validation, including product as well as market growth with different phases of technological meaning change. We argue that further steps are needed to develop drones from nice toys to professional tools—from photography and filming applications to inspection services and large cargo logistics. For innovation managers and entrepreneurs, we describe what triggers the emergence of a technology and attracts the needed actors to unleash its transformative potential. Our research is based on industry reports, news, and market studies as well as interviews with four industry actors.}
}
@article{GREEN2016454,
