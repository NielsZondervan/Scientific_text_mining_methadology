editor = {Laurence Melton and Fereidoon Shahidi and Peter Varelis},
booktitle = {Encyclopedia of Food Chemistry},
publisher = {Academic Press},
address = {Oxford},
pages = {188-195},
year = {2019},
isbn = {978-0-12-814045-1},
doi = {https://doi.org/10.1016/B978-0-08-100596-5.22334-2},
url = {https://www.sciencedirect.com/science/article/pii/B9780081005965223342},
author = {Roger Stanley and Chris Forbes-Ewan and Tracey McLaughlin},
keywords = {Military foods, MRE, Nutrition, Sustenance, Food stability, Shelf life, Oxidation},
abstract = {Military foods are needed to sustain defence forces while on deployment in the field whether on land, at sea or in the air. As they can be the sole source of nourishment, they have to provide adequate nutrients and energy to maintain optimum performance for the tasks being performed. The concepts for military foods date back to antiquity when large armies developed that had to take sufficient dried, salted or fermented foods to reliably feed their forces while on campaign. The era of modern stabilized military foods began with the advent of aseptic canning technology and evolved through the greater use of hermetically sealed food supply systems in WW1. Contemporary concepts of 24-h combat ration packs were instituted in WW2 to sustain soldiers going in to battle until field kitchens could be set up. Military logistics supply systems require rations to be stable for up to 3 years under non-refrigerated and potentially hot conditions. Solutions in food processing, chemistry and packaging have been developed to maintain the nutritive and vitamin content of the rations as well as the organoleptic acceptability.}
}
@article{PONBOON2016213,
