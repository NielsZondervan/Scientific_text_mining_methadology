abstract = {Recently a range of new information based technologies have been developed that create opportunities to implement more integrated and dynamic City Logistics solutions. Sensor networks and demand models can assist in advancing understanding and the prediction the negative impacts of freight on human health that can be used in evaluation. Advances in autonomous and electric freight vehicles have good potential for improving the efficiency and sustainability of urban freight systems. This paper presents an overview of how models can be used to promote the adoption of advanced technologies for enhancing City Logistics.}
}
@article{LI2020104585,
title = {Sharing economy to improve routing for urban logistics distribution using electric vehicles},
journal = {Resources, Conservation and Recycling},
volume = {153},
pages = {104585},
year = {2020},
issn = {0921-3449},
doi = {https://doi.org/10.1016/j.resconrec.2019.104585},
url = {https://www.sciencedirect.com/science/article/pii/S0921344919304914},
author = {Yan Li and Ming K Lim and Yingshuang Tan and Sir Yee Lee and Ming-Lang Tseng},
keywords = {Urban logistics, Sharing economy, Carbon tax, Electric vehicle routing, Ant colony algorithm},
abstract = {With the increasing concern on global warming, companies begin to use electric vehicles for urban distribution. Owing to battery capacity limitations, electric vehicles may need recharging during delivery process. Hence, the traditional vehicle routing methodologies, which were designed for fossil fuel-powered vehicles, are not feasible for electric vehicles. For this reason, this paper studies the urban logistics distribution routing problem of electric vehicles in the context of sharing economy with the consideration of carbon taxes (taxes on CO2 emissions) and time-of-use electricity prices (time-varying electricity prices), and proposes an optimization model of the electric vehicle routing problem in view of sharing economy. The proposed optimization model aims to minimize the total cost, which includes the operation, penalty, queuing (waiting at the charging stations), electricity and environment costs. An ant colony algorithm is developed for optimization purposes in the model. In this study, simulations are conducted based on a case study with actual locations. The first simulated scenario indicates that adopting distribution sharing is able to reduce travelling distances as well as the total cost in comparison to having separate distribution systems. The second simulated scenario is conducted under different time electricity prices; the results show that lower time-of-use electricity prices alleviate traffic congestion and reduce the total cost. In the third simulated scenario, the effects of different carbon taxes are analysed. The results show that the CO2 emissions will decrease when the carbon tax rate is increased. Finally, the limitations and future research directions of this paper are also discussed.}
}
@article{SHAHPARVARI2020106488,
title = {A GIS-LP integrated approach for the logistics hub location problem},
journal = {Computers & Industrial Engineering},
volume = {146},
pages = {106488},
year = {2020},
issn = {0360-8352},
doi = {https://doi.org/10.1016/j.cie.2020.106488},
url = {https://www.sciencedirect.com/science/article/pii/S0360835220302229},
author = {Shahrooz Shahparvari and Araz Nasirian and Alireza Mohammadi and Sepideh Noori and Prem Chhetri},
keywords = {Logistics Hub Location Problem, Heuristic (LHLP), Mixed Integer Quadratic constrained Programming (MIQCP), Geographic Information System (GIS), Multiple-criteria decision-making (MCDM), PROMETHEE},
abstract = {This study solves the location problem for establishing a logistics hub that serves as a consolidation center to distribute urban freight in the North Western Region of Iran. A novel multi-method approach is developed to implement a two-stage research framework. GIS-embedded multi-criteria decision tools are applied to identify potential regions; a K-means based heuristic approach is developed to select five potential sites; and PROMETHEE, multi-criteria decision-making tool, is implemented to ascertain the best possible site for establishing logistics hub. “Višekriterijumska Optimizacija i kompromisno Rešenje” (VIKOR) method is applied on multiple region-specific criteria to identify potential regions suitable for the logistics hub. A novel clustering Mixed Integer Quadratic constrained Programming (MIQCP) is then applied for clustering the centriods representing the most suitable sites.A K-means heuristic approach is also implemented as an alternative clustering solution approach to improve computational time for data processing. Several criteria such as cost, profit, health effects, political matters and regulations are then used to identify the best site using PROMETHEE method. The outputs of this study may inform evidence based decision-making help to solve highly complex, multi-scaled, and multi-criteria location problems. Municipal authorities, in conjunction with industry, can apply this framework to identify the best compromised site for establishing a regional logistics hub, which in turn help reduce costs, improve logistic efficiency and enhance regional performance.}
}
@article{PLAKAS20201680,
title = {Reverse Logistics of End-of-Life Plastics Using Industrial IoT and LPWAN Technologies – A Proposed Solution for the Bottled Water Industry},
