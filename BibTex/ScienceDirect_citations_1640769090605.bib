url = {https://www.sciencedirect.com/science/article/pii/B9780128123034000045},
author = {Bert Annevelink and Perttu Anttila and Kari Väätäinen and Benoît Gabrielle and Daniel García-Galindo and Sylvain Leduc and Igor Staritsky},
keywords = {Biomass supply chain, logistical component, logistical concept, design tools, optimization, simulation},
abstract = {This chapter will presents the current status and the main challenges of biomass logistics. Logistical aspects of the biomass supply chain will be delineated. It further provides a thorough description of different methodologies to design biomass value chains combined with relevant logistical assessment criteria. This includes also descriptions for how to integrate the various logistical components in modeling logistical tools that are currently available to implement the described methodologies. The chapter finishes with a set of case studies based on local data and made with these logistical tools.}
}
@article{SADLOWSKAWRZESINSKA20161761,
title = {Relations between stress potential of work features and occupational commitment of transport workers - in the context of optimization of logistics strategy of a company},
journal = {IFAC-PapersOnLine},
