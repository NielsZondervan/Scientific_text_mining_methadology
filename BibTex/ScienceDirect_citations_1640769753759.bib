abstract = {The paper describes the issues of using the RFID technology in transport and warehouse logistics. The features of RFID technology application in the system of logistics information systems as well as in the system of transportation and warehousing were analyzed. The advantages and disadvantages of its introduction were considered. As a result of the study, it was concluded that the application of the RFID technology would significantly increase the efficiency of logistics processes.}
}
@article{BUYUKOZKAN2021101197,
title = {Smart urban logistics: Literature review and future directions},
journal = {Socio-Economic Planning Sciences},
pages = {101197},
year = {2021},
issn = {0038-0121},
doi = {https://doi.org/10.1016/j.seps.2021.101197},
url = {https://www.sciencedirect.com/science/article/pii/S0038012121001890},
author = {Gülçin Büyüközkan and Öykü Ilıcak},
