number = {13},
pages = {1693-1698},
year = {2019},
note = {9th IFAC Conference on Manufacturing Modelling, Management and Control MIM 2019},
issn = {2405-8963},
doi = {https://doi.org/10.1016/j.ifacol.2019.11.444},
url = {https://www.sciencedirect.com/science/article/pii/S2405896319314259},
author = {A. Alfieri and A. De Marco and E. Pastore},
keywords = {Supply Chain Management, Supply chains, networks, Inventory control, production planning, scheduling, Modelling Supply Chain Dynamics, Transportation Systems, Optimisation Methods, Simulation Tools, Last Mile logistics, Fast Fashion supply chains},
abstract = {In the fast fashion industry, high delivery frequencies and low lead times are key factors of market competitiveness. To this goal, many companies are evaluating the possibility of opening urban satellites close to retail stores. Urban satellites can assure a number of daily replenishments larger than one at the retail stores. However, opening urban satellites requires expensive capital expenditures and can increase transportation costs. In this paper, we investigate the impact of urban satellites on the service level, inventory and transportation costs of fast fashion supply chains. Discrete event simulation is used to assess the value that urban satellites can provide to fast fashion supply chains. The application to an Italian retailer case study reveals that the increased transportation and storage costs may be compensated by increased sales as a result of a wider product assortment at the retail stores. The case study simulation results are proposed as possible lessons for other fast fashion last mile distribution systems.}
}
@incollection{JAIN202055,
title = {Chapter 3 - Transforming pharma logistics with the Internet of things},
editor = {Valentina Emilia Balas and Vijender Kumar Solanki and Raghvendra Kumar},
booktitle = {An Industrial IoT Approach for Pharmaceutical Industry Growth},
publisher = {Academic Press},
pages = {55-85},
year = {2020},
isbn = {978-0-12-821326-1},
doi = {https://doi.org/10.1016/B978-0-12-821326-1.00003-6},
url = {https://www.sciencedirect.com/science/article/pii/B9780128213261000036},
author = {Anjali Jain and Deepak Kumar Sharma},
keywords = {Pharma, IoT in pharma, supply chain, logistics, manufacturing, shipment tracking using the IoT, warehouse management, cold chain},
abstract = {The technology spectrum has lately expanded with a new innovation, the Internet of things (IoT), which is essentially a network of devices connected to the Internet. The IoT is gradually being adopted by several industries, bringing about major advancements in the accuracy, integrity, and security of their products and services. The pharmaceutical industry is one such industry where a lack of precision and inaccuracy can be fatal to its consumers, hence it needs to be ensured that incoming raw materials, processing, and the outgoing drugs and equipments are critically examined before being dispatched from the production warehouse. The IoT can certainly help solve this issue to a great extent. With the advent of the IoT, the pharmaceutical industry has been revolutionized as it facilitates efficiency in production, supply chain, logistics, and regulatory compliance. In the pharma industry there is always a risk of changes to environmental conditions which might harm the integrity of drugs, need regulating for the availability of stocks to avoid shortages, and making sure that the packaged drug has been received by the required customer. To ensure that the industry is safe from such recurring challenges, the IoT is being employed in manufacturing, warehouse management, and even shipment and tracking, to solve the age-old problems in this industry. In this chapter, we first cover the basics of the IoT and its applications in logistics and the supply chain. We also discuss the growth of the pharmaceutical industry, with a focus on the introduction of the IoT and how it has changed the face of pharma. The applications of the IoT in pharmaceutical logistics such as quality control, shipment tracking, and drug development are also discussed. The challenges faced in the industry and how they are being overcome with the IoT are also described. The aim of this chapter is to give readers an understanding of how the pharma industry has seen a technological boom with the IoT and how this has made the entire process, from drug development to shipment, extremely efficient and secure.}
}
