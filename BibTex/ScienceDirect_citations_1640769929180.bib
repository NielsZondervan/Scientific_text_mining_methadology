@article{BALMER2020850,
title = {The role of corporate brand image for B2B relationships of logistics service providers in China},
journal = {Journal of Business Research},
volume = {117},
pages = {850-861},
year = {2020},
issn = {0148-2963},
doi = {https://doi.org/10.1016/j.jbusres.2020.03.043},
