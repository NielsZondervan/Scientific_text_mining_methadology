@article{ALARFAJ20182182,
title = {Comparison of drug administration logistics between prothrombin complex concentrates and plasma in the emergency department},
journal = {The American Journal of Emergency Medicine},
volume = {36},
number = {12},
pages = {2182-2186},
year = {2018},
issn = {0735-6757},
doi = {https://doi.org/10.1016/j.ajem.2018.03.064},
